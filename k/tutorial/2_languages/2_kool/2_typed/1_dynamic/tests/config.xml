<tests>

  <test definition="kool-typed-dynamic.k"
        programs="../../programs/"
        extension="kool"
        exclude="cycle"
        results="." >
        <kompile-option name="--backend" value="java" />
        <all-programs>
          <krun-option name="--output" value="none" />
          <krun-option name="--smt" value="none" />
        </all-programs>
  </test>

  <test definition="kool-typed-dynamic.k"
        programs="../../programs/"
        extension="kool"
        exclude="cycle"
        results="." >
        <kompile-option name="--backend" value="java" />
        <all-programs>
          <krun-option name="--output" value="none" />
          <krun-option name="--pattern-matching" />
        </all-programs>
  </test>

</tests>
